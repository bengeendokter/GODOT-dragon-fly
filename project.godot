config_version=4

_global_script_classes=[ {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
}, {
"base": "ParallaxLayer",
"class": "Parallax",
"language": "GDScript",
"path": "res://main/Parallax.gd"
} ]
_global_script_class_icons={
"GitAPI": "",
"Parallax": ""
